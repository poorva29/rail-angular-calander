@model IEnumerable<MiraiConsultMVC.Models.Report>
<tbody id="DoctorsReoprt">
     @if (Model != null && Model.ToList().Count > 0 )
            { 
    foreach(Report repObj in Model)
    {
   
    <tr>
        <td class="column-word-break">
            @repObj.name
        </td>
        <td class="column-word-break">
            @repObj.city
        </td>
        <td class="column-word-break">
            @repObj.location
        </td>
        <td class="column-word-break">
            @repObj.appointmentbooked
        </td>
        <td class="column-word-break">
            @repObj.appointmentclicked
        </td>
        <td class="column-word-break">
            @repObj.avgresponsetime
        </td>
    </tr>
    }
    } 
</tbody>